{
  "version": 2,
  "builds": [
    {
      "src": "api/mistral.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/solve",
      "dest": "/api/mistral.js"
    },
    {
      "src": "/check-answer",
      "dest": "/api/mistral.js"
    },
    {
      "src": "/generate-plan",
      "dest": "/api/mistral.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/mistral.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}